
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Areas/Customer/Views/CustomerLayoutPartial/_CustomerHeaderPartial.cshtml")
<body>
	<div class="container-scroller">
		@await Html.PartialAsync("/Areas/Customer/Views/CustomerLayoutPartial/_CustomerNavbarPartial.cshtml")
		<div class="container-fluid page-body-wrapper">
			@await Html.PartialAsync("/Areas/Customer/Views/CustomerLayoutPartial/_CustomerSidebarPartial.cshtml")
			<div class="main-panel">
				<div class="content-wrapper">
					@RenderBody()
				</div>
				@await Html.PartialAsync("/Areas/Customer/Views/CustomerLayoutPartial/_CustomerFooterPartial.cshtml")
			</div>
		</div>
	</div>
	@await Html.PartialAsync("/Areas/Customer/Views/CustomerLayoutPartial/_CustomerScriptsPartial.cshtml")
</body>
</html>
