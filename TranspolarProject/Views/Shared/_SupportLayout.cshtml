
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Areas/Support/Views/SupportLayoutPartial/_SupportHeaderPartial.cshtml")
<body>
	<div class="container-scroller">
		@await Html.PartialAsync("/Areas/Support/Views/SupportLayoutPartial/_SupportNavbarPartial.cshtml")
		<div class="container-fluid page-body-wrapper">
			@await Html.PartialAsync("/Areas/Support/Views/SupportLayoutPartial/_SupportSidebarPartial.cshtml")
			<div class="main-panel">
				<div class="content-wrapper">
					@RenderBody()
				</div>
				@await Html.PartialAsync("/Areas/Support/Views/SupportLayoutPartial/_SupportFooterPartial.cshtml")
			</div>
		</div>
	</div>
	@await Html.PartialAsync("/Areas/Support/Views/SupportLayoutPartial/_SupportScriptsPartial.cshtml")
</body>
</html>
